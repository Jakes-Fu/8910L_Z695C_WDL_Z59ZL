# Language definition for specific LP
ifneq ($(strip $(VARIANT_PID)),)
$(foreach language,$(LANGUAGE_SUPPORT_ALL),$(call language_support_clear,$(language)))
$(foreach language,$(LANGUAGE_$(word 1,$(subst _,$(SPACE),$(strip $(VARIANT_PID))))_SUPPORT),$(call language_support_set,$(language)))
ifeq ($(word 1,$(subst _,$(SPACE),$(strip $(VARIANT_PID)))),QA)
#RES_FONT_SUPPORT_POINTARRAY = TRUE
RES_FONT_SUPPORT_POINTARRAY = FALSE
else
RES_FONT_SUPPORT_POINTARRAY = FALSE
endif 

ifneq ($(VARIANT_PID),IX_23ZOU3100MA)
RES_RINGTONE_GLOBAL = TRUE
else
RES_RINGTONE_GLOBAL = FALSE
endif
